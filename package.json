{
  "name": "connorbaer.co",
  "version": "5.0.0",
  "description": "Website for Connor Bär.",
  "main": "app.js",
  "author": "Connor Bär, connor-baer <hello@connorbaer.co>",
  "copyright": "connorbaer",
  "license": "MIT",
  "homepage": "https://github.com/connor-baer/connorbaer.co",
