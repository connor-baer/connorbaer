    "developer",
    "designer"
  ],
  "bugs": {
    "url": "https://github.com/connor-baer/connorbaer.co/issues"
  },
  "scripts": {
    "dev": "webpack --env=develop --display-error-details",
    "build": "run-p build:client build:server",
    "build:client": "next build src",
    "build:server": "webpack --env=production",
    "start": "NODE_ENV=production node dist/app.js",
    "export": "next export src",
    "deploy": "node -r esm scripts/deploy.js",
    "now-build": "yarn build",
    "clean": "rm -rf ./dist",
    "lint": "run-p lint:client lint:server",
    "lint:client": "foundry run eslint --fix src/**/*.js",
    "lint:server": "foundry run eslint --fix server/**/*.js",
    "analyze": "BUNDLE_ANALYZE=both yarn build:client",
    "create:component": "foundry run plop component"
  },
  "dependencies": {
